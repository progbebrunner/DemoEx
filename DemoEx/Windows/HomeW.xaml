<Window x:Class="DemoEx.Windows.HomeW"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoEx.Windows"
        mc:Ignorable="d"
        Title="Приложение" Height="777" Width="1234">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="700"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="HomeWFr" Grid.Row="0" NavigationUIVisibility="Hidden" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Button x:Name="btnBack" Grid.Row="1" Style="{StaticResource BscBtn}" VerticalAlignment="Top" Content="Назад" Click="btnBack_Click"/>
    </Grid>
</Window>
